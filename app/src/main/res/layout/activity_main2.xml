<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity2">

    <TextView
        android:id="@+id/totalNum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="64dp"
        android:layout_marginBottom="12dp"
        android:text="$0.00"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/totalText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="46dp"
        android:layout_marginBottom="12dp"
        android:text="@string/total"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/totalNum"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/taxText" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="179dp"
        android:layout_height="64dp"
        android:layout_marginTop="50dp"
        android:text="@string/repair_order"
        android:textAlignment="center"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/orderType"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Order Type"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="56dp"
        tools:layout_editor_absoluteY="207dp" />

    <TextView
        android:id="@+id/labor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Labor"
        android:textStyle="bold"
        app:layout_constraintBaseline_toBaselineOf="@+id/laborEdit"
        tools:layout_editor_absoluteX="68dp" />

    <TextView
        android:id="@+id/technician"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Technician"
        android:textStyle="bold"
        app:layout_constraintBaseline_toBaselineOf="@+id/technicianEdit"
        tools:layout_editor_absoluteX="56dp" />

    <TextView
        android:id="@+id/inspection"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Inspection"
        android:textStyle="bold"
        app:layout_constraintBaseline_toBaselineOf="@+id/inspectionEdit"
        tools:layout_editor_absoluteX="56dp" />

    <TextView
        android:id="@+id/Paint"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="72dp"
        android:text="Paint"
        android:textStyle="bold"
        app:layout_constraintBaseline_toBaselineOf="@+id/PaintEdit"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/laborEdit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="34dp"
        android:layout_marginEnd="16dp"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/partsEdit"
        app:layout_constraintTop_toBottomOf="@+id/partsEdit" />

    <EditText
        android:id="@+id/PaintEdit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="29dp"
        android:layout_marginEnd="15dp"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/inspectionEdit"
        app:layout_constraintTop_toBottomOf="@+id/inspectionEdit" />

    <EditText
        android:id="@+id/inspectionEdit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="16dp"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/technicianEdit"
        app:layout_constraintTop_toBottomOf="@+id/technicianEdit" />

    <EditText
        android:id="@+id/orderTypeEdit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintBaseline_toBaselineOf="@+id/orderType"
        tools:layout_editor_absoluteX="186dp" />

    <EditText
        android:id="@+id/technicianEdit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="39dp"
        android:layout_marginEnd="16dp"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/orderTypeEdit"
        app:layout_constraintTop_toBottomOf="@+id/orderTypeEdit" />

    <EditText
        android:id="@+id/partsEdit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintStart_toStartOf="@+id/PaintEdit"
        tools:layout_editor_absoluteY="505dp" />

    <TextView
        android:id="@+id/Parts"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Parts"
        android:textStyle="bold"
        app:layout_constraintBaseline_toBaselineOf="@+id/partsEdit"
        tools:layout_editor_absoluteX="72dp" />

    <TextView
        android:id="@+id/subtotalText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="47dp"
        android:text="Subtotal"
        android:textAlignment="center"
        app:layout_constraintEnd_toStartOf="@+id/subtotalNum"
        app:layout_constraintTop_toBottomOf="@+id/laborEdit" />

    <TextView
        android:id="@+id/taxText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="76dp"
        android:text="Tax"
        android:textAlignment="center"
        app:layout_constraintBottom_toTopOf="@+id/totalText"
        app:layout_constraintEnd_toStartOf="@+id/taxNum"
        app:layout_constraintTop_toBottomOf="@+id/subtotalText"
        app:layout_constraintVertical_bias="0.969" />

    <TextView
        android:id="@+id/subtotalNum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="64dp"
        android:text="$0.00"
        app:layout_constraintBottom_toTopOf="@+id/taxNum"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/taxNum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="64dp"
        android:text="$0.00"
        app:layout_constraintBottom_toTopOf="@+id/totalNum"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/submitButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Submit"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="652dp" />

</androidx.constraintlayout.widget.ConstraintLayout>